<Application x:Class="ProcessAutomation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProcessAutomation"
             StartupUri="ShellView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Zoranof.UI.Wpf;component/Themes/Purple.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Zoranof.UI.Wpf;component/Themes/Light.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Zoranof.UI.Wpf;component/DefaultStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Zoranof.UI.Wpf;component/Resources.xaml" />


            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
